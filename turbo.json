{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "types": {
      "dependsOn": ["^build"]
    },
    "lint": {},
    "test": {},
    "coverage": {
      "outputs": ["coverage/**"]
    },
    "build": {
      "outputs": ["dist/**", "build/**"],
      "dependsOn": ["^build"]
    },
    "dev": {
      "cache": false
    }
  }
}
