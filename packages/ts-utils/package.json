{
  "name": "@cloudwalker/ts-utils",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "build": "tsup src/main.ts",
    "dev": "tsup src/main.ts --watch"
  },
  "files": [
    "dist/**"
  ],
  "main": "./dist/main.cjs",
  "module": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/main.js",
      "require": "./dist/main.cjs"
    }
  },
  "sideEffects": false,
  "author": {
    "name": "Luca Barone",
    "email": "baro.luc@gmail.com",
    "url": "https://github.com/cloud-walker"
  },
  "devDependencies": {
    "@vitest/coverage-c8": "^0.25.3",
    "tsup": "^6.2.3",
    "typescript": "^4.8.2",
    "vitest": "^0.25.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
