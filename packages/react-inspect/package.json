{
  "name": "@cloudwalker/react-inspect",
  "version": "0.5.2",
  "description": "Because json stringify is not enough",
  "scripts": {
    "lint": "eslint src/**",
    "test": "jest --watch",
    "coverage": "jest --coverage",
    "storybook": "start-storybook -p 9000 -c .storybook",
    "build": "tsup src/main.js",
    "precommit": "lint-staged"
  },
  "files": [
    "dist/**"
  ],
  "main": "dist/main.js",
  "module": "dist/main.mjs",
  "lint-staged": {
    "*.js": [
      "prettier",
      "eslint",
      "git add"
    ]
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "author": "Luca Barone <baro.luc@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/cloud-walker/workbench"
  },
  "bugs": {
    "url": "https://github.com/cloud-walker/workbench/issues"
  },
  "keywords": [
    "react",
    "inspect",
    "log",
    "json",
    "stringify"
  ],
  "peerDependencies": {
    "prop-types": ">=15",
    "react": ">=15"
  },
  "devDependencies": {
    "@storybook/react": "^3.2.8",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^2.0.0",
    "eslint": "^4.7.0",
    "eslint-plugin-react": "^7.3.0",
    "husky": "^0.14.3",
    "jest": "^21.0.2",
    "lint-staged": "^4.2.1",
    "np": "^2.16.0",
    "prettier": "^1.5.3",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.6.1",
    "sanitize.css": "^5.0.0",
    "tsup": "^6.2.3"
  },
  "dependencies": {
    "just-is-circular": "^1.0.0",
    "ramda": "^0.24.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
