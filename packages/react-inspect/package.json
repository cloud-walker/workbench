{
  "name": "@cloudwalker/react-inspect",
  "version": "0.9.0",
  "description": "Because json stringify is not enough",
  "scripts": {
    "types": "tsc --noEmit",
    "lint": "eslint --ignore-path=\"../../.gitignore\" .",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "stories": "start-storybook -p 9000 -c .storybook",
    "build": "tsup src/main.ts",
    "dev": "tsup src/main.ts --watch"
  },
  "files": [
    "dist/**"
  ],
  "main": "dist/main.js",
  "module": "dist/main.mjs",
  "types": "./dist/main.d.ts",
  "sideEffects": false,
  "author": {
    "name": "Luca Barone",
    "email": "baro.luc@gmail.com",
    "url": "https://github.com/cloud-walker"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cloud-walker/workbench"
  },
  "bugs": {
    "url": "https://github.com/cloud-walker/workbench/issues"
  },
  "keywords": [
    "react",
    "inspect",
    "log",
    "json",
    "stringify"
  ],
  "peerDependencies": {
    "react": ">=16.8"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/eslint-parser": "^7.21.3",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.4",
    "@cloudwalker/react-utils": "*",
    "@storybook/react": "^6.5.16",
    "@testing-library/react": "^13.3.0",
    "@types/react": "^18.0.35",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "@vitejs/plugin-react": "^3.1.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react": "^7.32.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sanitize.css": "^13.0.0",
    "tsup": "^6.7.0",
    "typescript": "^4.9.5",
    "vitest": "^0.26.3"
  },
  "dependencies": {
    "just-is-circular": "^2.1.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
